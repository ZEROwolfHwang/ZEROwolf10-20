<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="@dimen/layout_margin10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:elevation="3dp"
            android:gravity="center">

            <ImageView
                style="@style/main_left_image_view"
                android:background="@drawable/image_station"/>

            <LinearLayout
                style="@style/main_text_layout_bg_station_style">

                <TextView
                    style="@style/main_text_static_tv_style"
                    android:text="登记站"/>

                <ImageView
                    style="@style/main_text_static_iv_style"/>

                <TextView
                    android:id="@+id/tv_change_station_main"
                    style="@style/main_text_change_tv_style"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="@dimen/layout_margin5dp"
            android:elevation="3dp"
            android:gravity="center">

            <ImageView
                style="@style/main_left_image_view"
                android:background="@drawable/image_lane"/>

            <LinearLayout
                style="@style/main_text_layout_bg_else_style">

                <TextView
                    style="@style/main_text_static_tv_style"
                    android:text="登记车道"/>

                <ImageView
                    style="@style/main_text_static_iv_style"/>

                <TextView
                    android:id="@+id/tv_change_lane_main"
                    style="@style/main_text_change_tv_style"/>

            </LinearLayout>

        </LinearLayout>

        <include layout="@layout/layout_check_operator"/>

        <include layout="@layout/layout_login_operator"/>


    </LinearLayout>

    <Button
        android:layout_margin="@dimen/layout_margin10dp"
        android:id="@+id/btn_enter_show"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginBottom="@dimen/layout_margin10dp"
        android:background="@drawable/shape_main_to_show_btn"
        android:elevation="@dimen/layout_margin10dp"
        android:text="开始采集"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:background="#f0f0f0"
        android:layout_height="@dimen/layout_margin0dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:padding="@dimen/layout_margin10dp"
            android:orientation="horizontal">

            <RelativeLayout
                android:id="@+id/custom_shape_line_rect_has"
                android:layout_width="110dp"
                android:background="#ffffff"
                android:elevation="3dp"
                android:layout_height="100dp">

                <TextView
                    android:id="@+id/tv_math_number_main_has"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="100"
                    android:textColor="#f64545"
                    android:textSize="@dimen/text24"/>
                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:text="保存草稿"
                    android:layout_marginBottom="@dimen/layout_margin10dp"
                    android:textColor="#808080"
                    android:textSize="@dimen/text14"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="110dp"
                android:id="@+id/custom_shape_line_rect_has_not"
                android:layout_marginLeft="@dimen/layout_margin10dp"
                android:layout_marginRight="@dimen/layout_margin10dp"
                android:background="#ffffff"
                android:elevation="3dp"
                android:layout_height="100dp">
                <TextView

                    android:layout_width="wrap_content"
                    android:layout_marginBottom="@dimen/layout_margin10dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:text="已提交内容"
                    android:textColor="#808080"
                    android:textSize="@dimen/text14"/>

                <TextView
                    android:id="@+id/tv_math_number_main_has_not"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="100"
                    android:textColor="#f64545"
                    android:textSize="@dimen/text24"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="110dp"
                android:background="#ffffff"
                android:elevation="3dp"
                android:layout_height="100dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="100"
                    android:textColor="#f64545"
                    android:textSize="@dimen/text24"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="@dimen/layout_margin10dp"
                    android:text="黑名单展示"
                    android:textColor="#808080"
                    android:textSize="@dimen/text14"/>

            </RelativeLayout>


        </LinearLayout>
    </LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:background="#d8d8d8"
    android:layout_height="35dp">

    <LinearLayout
        android:layout_width="@dimen/layout_margin0dp"
        android:layout_weight="2"
        android:background="#b3b3b3"
        android:layout_height="match_parent">
        <TextView
            android:layout_width="wrap_content"
            android:layout_marginLeft="@dimen/layout_margin10dp"
            android:drawableLeft="@drawable/main_room_left_img"
            android:text="16G"
            android:textSize="20sp"
            android:gravity="center_vertical"
            android:textColor="#4d4d4d"
            android:layout_height="wrap_content"/>
        <TextView
            android:gravity="center_vertical"
            android:layout_width="wrap_content"
            android:textColor="#4d4d4d"
            android:text="/24G"
            android:textSize="20sp"
            android:layout_height="wrap_content"/>
        
    </LinearLayout>
    <TextView
        android:layout_width="@dimen/layout_margin0dp"
        android:layout_weight="1"
        android:layout_height="match_parent"/>
</LinearLayout>
</LinearLayout>
